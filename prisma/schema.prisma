// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Patient {
  id Int
  name String
  surname String
  birth Date
  cpf String
  email String
  phone String
  address String
  allergy String
  medical_record

  CreatedAt DateTime
  updateAt DateTime
}
