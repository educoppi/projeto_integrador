// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}


model Historic {
  id int

  appointmentDate DateTime
  ExamId int
  medicationId int
  receitaId int

  createdAt DateTime
  updatedAt DateTime
}

model Exam {
  id int

  date Date
  type String
  result String
  observation String

  createdAt DateTime
  updatedAt DateTime
}