// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Medication {
  id Int
  createdAt DateTime
  updatedAt DateTime

  name String
  quantity String
  type String
  expiresAt DateTime

}

model MedicationMovement {
  id Int
  createdAt DateTime
  updatedAt DateTime

  medicationId Int
  date Date
  quantity String 
  userId Int
}

